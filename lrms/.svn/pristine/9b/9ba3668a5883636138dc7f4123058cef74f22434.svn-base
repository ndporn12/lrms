<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h3>실습실 등록</h3>
	<form action="/lab" method="POST" enctype="multipart/form-data">
		<table>
			<input type="hidden" name="state" value='A'>
			<tr>
				<td>호실</td>
				<td><input type="text" name="no" size="40"></td>
				<td><input type="button" name="" value="버튼"></td>
			</tr>
			<tr>
				<td>정원</td>
				<td><input type="text" name="acceptablePerson" size="40"></td>
			</tr>
			<tr>
				<td>장비 수</td>
				<td><input type="text" name="availableEquipNo" size="40"></td>
			</tr>
			<tr>
				<td>용도</td>
				<td><input type="text" name="usePerpose" size="40"></td>
			</tr>
			<tr>
				<td>컴퓨터 사양</td>
				<td><input type="text" name="computerSpec" size="40"></td>
			</tr>
			<tr>
				<td>사진</td>
				<td><input type="file" value="사진 선택" name="multipartFile"></td>
			</tr>
		</table>
		</br>
		<input type="submit" value="등록">
	</form>
</body>
</html>